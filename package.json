{
	"name": "rueso-library",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "bun run build:client && bun run build:server",
		"build:noExternal": "bun run build:client && bun run build:server:noExternal",
		"build:client": "vite build --outDir dist/client",
		"build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
		"build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.ts --outDir dist/server",
		"generate": "vite build --outDir dist/static && bun run build:server && bun prerender",
		"serve": "NODE_ENV=production bun serverElysia",
		"serve-express": "NODE_ENV=production bun serverExpress",
		"serve-dev": "bun serverElysia",
		"debug": "bun --inspect-brk serverElysia",
		"type-check": "vue-tsc --build"
	},
	"dependencies": {
		"@elysiajs/static": "^1.4.4",
		"@fortawesome/fontawesome-svg-core": "^7.1.0",
		"@fortawesome/free-solid-svg-icons": "^7.1.0",
		"@fortawesome/vue-fontawesome": "^3.1.2",
		"@popperjs/core": "^2.11.8",
		"@tanstack/vue-query": "^5.90.5",
		"@types/bootstrap": "^5.2.10",
		"@types/bun": "^1.3.1",
		"@types/compression": "^1.8.1",
		"@types/express": "^5.0.5",
		"@unhead/ssr": "1.11.20",
		"@unhead/vue": "1.11.20",
		"@vueuse/core": "^14.0.0",
		"bootstrap": "^5.3.8",
		"compression": "^1.8.1",
		"datatables.net-bs5": "^2.3.4",
		"datatables.net-vue3": "^3.0.4",
		"elysia": "^1.4.13",
		"json-server": "^1.0.0-beta.3",
		"sass": "^1.93.2",
		"sirv": "^3.0.2",
		"vue": "^3.5.22",
		"vue-router": "^4.6.3"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.2",
		"@types/jsdom": "^27.0.0",
		"@types/node": "^24.9.2",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.8.1",
		"jiti": "^2.6.1",
		"jsdom": "^27.0.1",
		"npm-run-all2": "^8.0.4",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vite-plugin-vue-devtools": "^8.0.3",
		"vue-tsc": "^3.1.2"
	}
}
