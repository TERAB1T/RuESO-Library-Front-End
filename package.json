{
	"name": "rueso-library",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "bun run build:client && bun run build:server",
		"build:noExternal": "bun run build:client && bun run build:server:noExternal",
		"build:client": "vite build --outDir dist/client",
		"build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
		"build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.ts --outDir dist/server",
		"generate": "vite build --outDir dist/static && bun run build:server && bun prerender",
		"serve": "NODE_ENV=production bun serverElysia",
		"serve-express": "NODE_ENV=production bun serverExpress",
		"serve-dev": "bun serverElysia",
		"debug": "bun --inspect-brk serverElysia",
		"type-check": "vue-tsc --build"
	},
	"dependencies": {
		"@elysiajs/static": "^1.2.0",
		"@popperjs/core": "^2.11.8",
		"@tanstack/vue-query": "^5.68.0",
		"@types/bootstrap": "^5.2.10",
		"@types/bun": "^1.2.5",
		"@types/compression": "^1.7.5",
		"@types/express": "^5.0.0",
		"@unhead/ssr": "^1.11.18",
		"@unhead/vue": "^1.11.18",
		"bootstrap": "^5.3.3",
		"compression": "^1.8.0",
		"datatables.net-bs5": "^2.2.2",
		"datatables.net-vue3": "^3.0.4",
		"elysia": "^1.2.25",
		"express": "^5.0.1",
		"json-server": "^1.0.0-beta.3",
		"sass": "^1.84.0",
		"sirv": "^3.0.1",
		"vue": "^3.5.13",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.13.1",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.7.0",
		"jiti": "^2.4.2",
		"jsdom": "^26.0.0",
		"npm-run-all2": "^7.0.2",
		"typescript": "~5.7.3",
		"vite": "^6.0.11",
		"vite-plugin-vue-devtools": "^7.7.1",
		"vitest": "^3.0.5",
		"vue-tsc": "^2.2.0"
	}
}
